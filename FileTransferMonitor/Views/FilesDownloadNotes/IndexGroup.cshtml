@model IQueryable<IGrouping<string, FileTransferMonitor.Models.FilesDownloadNote>>
@{
    ViewBag.Title = "IndexGroup";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>IndexGroup</title>
</head>
<body>
    <label class="span_report">.</label>
    <label class="label_report">Create report - click ~~></label>
    <button type="button" id="Create-report-excell-file" class="btn-default">
        <img class="img_excell" src="data:image/png;base64," />
    </button>
        
    @foreach (IGrouping<string, FileTransferMonitor.Models.FilesDownloadNote> item in Model)
    {
        <h4 style="text-align:center;text-decoration:underline;background-color:silver;"><text style="color:blue;"> User operator: </text>@item.Key</h4>

        IEnumerable<IGrouping<string, FileTransferMonitor.Models.FilesDownloadNote>> itemgroup = item.GroupBy(g => g.FileOperations);
        foreach (IGrouping<string, FileTransferMonitor.Models.FilesDownloadNote> ig in itemgroup)
        {
            <ul class="btn-default">
                <text style="color:dodgerblue;margin-left:70px; text-decoration:underline;"> Files operations:</text> @ig.Key

                @foreach (FileTransferMonitor.Models.FilesDownloadNote i in ig)
                {
                    <li><text style="color:chocolate;">Datetime operation :</text>  @i.DateTimeDownload </li>
                    <li><text style="color:chocolate;">File name :</text> @i.FilesUploadNote.Name</li>
                    <li>
                        <text style="color:chocolate;">File size Mb :</text> @i.FilesUploadNote.Size
                        <ul>
                            <li>
                                <text style="color:coral;">User upload file : </text>@i.FilesUploadNote.UserName
                                <text style="color:coral; margin-left: 12px;">DateTime : </text>@i.FilesUploadNote.DataTimeUpload
                            </li>
                        </ul>
                    </li>
                }
            </ul>
        }
    }
</body>
</html>
